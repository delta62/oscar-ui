{
  "name": "oscar-ui",
  "version": "1.0.4",
  "description": "Frontend application for an Oscars trivia game",
  "scripts": {
    "start": "concurrently \"node-sass app/ -o output\" \"node-sass app/ -o output -wr \" \"tsc -w\" \"lite-server\"",
    "prepublish": "npm run sass:prod && npm run ngc && npm run rollup && npm run copysass && npm run copyhtml",
    "sass:prod": "node-sass app/ -o app --output-style compressed",
    "copysass": "cp app/styles/app.css dist/",
    "copyhtml": "cp index-aot.html dist/index.html",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js"
  },
  "files": [
    "dist/"
  ],
  "repository": "git@github.com:delta62/oscar-ui.git",
  "author": "Sam Noedel <sam.noedel@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "oscar-resources": "^1.0.7"
  },
  "devDependencies": {
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/compiler-cli": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/platform-server": "2.4.7",
    "@angular/router": "3.4.7",
    "@types/node": "^7.0.5",
    "@types/socket.io-client": "^1.4.29",
    "concurrently": "^3.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "flux-lite": "^3.0.0",
    "lite-server": "^2.2.2",
    "node-sass": "^4.4.0",
    "reflect-metadata": "^0.1.9",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "^5.0.1",
    "socket.io-client": "^1.7.2",
    "systemjs": "0.20.7",
    "typescript": "^2.1.4",
    "zone.js": "^0.7.6"
  }
}
